<h1>My List of Colleges</h1>

  <ul>
  <% @colleges.each do |college| %>
     <li><a href="/college/<%= college.id %>"><%= college.name %></a></li>


    <% if college.name%>
    College Name: <%= college.name %>
    Region: <%= college.region %>
    Student Name: <%=current_student %>
    <% end %>
  <% end %>
